    <script src="~/Scripts/knockout-3.4.2.js"></script>
    <script src="~/Scripts/knockout.validation1.min.js"></script>
    <script src="~/assets/scripts/core/metronic.js"></script>
    <script src="~/assets/scripts/core/layout.js"></script>


    <script src="https://www.amcharts.com/lib/4/core.js"></script>
    <script src="https://www.amcharts.com/lib/4/charts.js"></script>
    <script src="https://www.amcharts.com/lib/4/themes/animated.js"></script>
    
    
   
</head>
<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
    <div class="page-wrapper">
        <!-- BEGIN HEADER -->
        <div class="page-header navbar navbar-fixed-top">
            <div class="page-header-inner">
                <div class="page-logo">
                    <img src="https://www.cisin.com/images/logo2.png" alt="logo" class="logo-default" style="height:36px;width:60px;margin: 8px 0 0;"/>
                </div>

            </div>

            <div style="float:right">
           <a class="btn btn-large btn-primary"  href="@Url.Action("LogOut", "Login")"><span class="text">LogOut</span></a>
            </div>
        </div>
        <!-- END HEADER -->
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <!-- BEGIN CONTAINER -->
        <div class="page-container">
            <div id="my-dialog"></div>
            <div class="page-sidebar-wrapper">
                <div class="page-sidebar navbar-collapse collapse">
                    <ul class="page-sidebar-menu  page-header-fixed " data-keep-expanded="false"
                        data-auto-scroll="true" data-slide-speed="200" style="padding-top: 20px">
                        <!-- BEGIN SIDEBAR TOGGLER BUTTON -->
                        <li class="sidebar-toggler-wrapper hide">
                            <div class="sidebar-toggler">
                                <span></span>
                            </div>
                        </li>
                        
                        <!-- END SIDEBAR TOGGLER BUTTON -->
                        @Html.Partial("~/Views/Shared/Menus/_DefaultMenu.cshtml")
                        
                    </ul>
                </div>
            </div>
            <div class="page-content-wrapper">
                <div class="page-content">
                    @RenderBody()
   
                </div>
            </div>
        </div>
        <!-- END CONTAINER -->
        <!-- BEGIN FOOTER -->
        <div class="page-footer">
            <div class="page-footer-inner">OnRamp</div>
            <div class="scroll-to-top">
                <i class="icon-arrow-up"></i>
            </div>
        </div>
        <!-- END FOOTER -->
    </div>
    <style type="text/css">
        .page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu .sub-menu > li:hover > a, .page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu .sub-menu > li.open > a, .page-sidebar-closed.page-sidebar-fixed .page-sidebar:hover .page-sidebar-menu .sub-menu > li.active > a,
        .page-sidebar .page-sidebar-menu .sub-menu > li:hover > a,
        .page-sidebar .page-sidebar-menu .sub-menu > li.open > a,
        .page-sidebar .page-sidebar-menu .sub-menu > li.active > a {
            /*background: #e98c1a !important;*/
            border-top-color: transparent;
            color: white;
        }

        ul.sub-menu .subClass.active {
            position: relative;
        }

        .sub-menu .subClass.active .selected {
            background-image: url('../../assets/img/sidebar-menu-arrow.png');
            display: block;
            float: right;
            height: 25px;
            position: absolute;
            right: 0;
            top: 2px;
            width: 8px;
        }
    </style>
    <script type="text/javascript">

        $(document).ready(function () {
            Metronic.init();
            Layout.init();
            $.ajaxSetup({ cache: false });
            $("form").submit(function () { }).change(function () { });
        });
    </script>

</body>
</html>